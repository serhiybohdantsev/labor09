#include <iostream>
#include <math.h>
using namespace std;

int main()
{
  float sum, x, y, n, dob, m, z;
  cout << "Input value x for Y\nz = ";
  cin >> x;
  sum = 0;
  for (int n = 1; n < 6; n++) 
  {
    sum += log(n);
  }
  y = pow(x,2) + sum;
  cout << "The evaluation results Y = " << y << endl;

  dob = 1;
  cout << "\nInput value x for Z\nx = ";
  cin >> x;
  for (int m= 1; m<= 5; m++)
  {
    dob *= tan(i*x);
  }
  z = dob + sqrt(x);
  cout << "The evaluation results Z = " << z << endl;
  system("pause");
	  return 0;

} 
