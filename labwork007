#include <iostream>
#include <math.h>
using namespace std;

int main()
{
  float sum, x, y, i, dob, j, z;
  cout << "Input value x for Y\nz = ";
  cin >> x;
  sum = 0;
  for (int i = 1; i < 6; i++) 
  {
    sum += log(i);
  }
  y = pow(x,2) + sum;
  cout << "The evaluation results Y = " << y << endl;

  dob = 1;
  cout << "\nInput value x for Z\nx = ";
  cin >> x;
  for (int j= 1; j<= 5; j++)
  {
    dob *= tan(i*x);
  }
  z = dob + sqrt(x);
  cout << "The evaluation results Z = " << z << endl;
  system("pause");
	  return 0;

}